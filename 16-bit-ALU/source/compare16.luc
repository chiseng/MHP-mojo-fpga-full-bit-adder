module compare16 (
    input alufn[6],  // ALUFN[0:5]
    input z,
    input v,
    input n,
    output cmp[1]
  ) {

  always {
    case(alufn[2:1])
      {
      b01: // LSB = z
        cmp[0] = z;
      b10: // LSB = n xor v
        cmp[0] = n ^ v;
      b11:
        cmp[0] = z + (n ^ v);
      default:
        cmp[0] = 0;
    }
    
  }
}
