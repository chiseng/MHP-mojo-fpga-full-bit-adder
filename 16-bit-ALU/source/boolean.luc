module boolean (
    output out[16],
    input alufn[6],
    input a[16],
    input b[16]
    
    
  ) {

  always {
    out = 16hxx;
    case(alufn){
      b011000: 
      out=a&b ;
      
      b011110:
      out= a|b;
      
      b010110:
      out= a^b;
      
      b011010:
      out=a;
      
    
    }
  }
}
