 module multiply (
    input alufn[6],
    input a[16],
    input b[16],
    output out[16]
  ) {

  always {
  
    // initialise output array
    out = 0;
    
    case (alufn[5:0]){
      // if alufn is 000010 => Multiply a with b    
      b000010:
        out = a * b;
      
      // if alufn is 000011 => Divide a by b
      b000011:
        out = a / b;
  }
}
}